#include<bits/stdc++.h>
using namespace std;
int main(){
	int t;cin>>t;
	cin.ignore();
	while(t--){
		string s;
		getline(cin,s);
		stringstream ss(s);
		vector<string> v;
		string word;
		while(ss>>word){
			reverse(word.begin(),word.end());
			v.push_back(word+" ");
		} 
		for(auto x:v) cout<<x;
		cout<<endl;
	}
}
